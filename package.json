{
  "name": "e-clock",
  "version": "0.0.0",
  "description": "Chrome extension to replace \"new tab\" with digital clock",
  "repository": {
    "type": "git",
    "url": "https://github.com/husa/e-clock"
  },
  "author": "Yuriy Husnay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husa/e-clock/issues"
  },
  "homepage": "https://github.com/husa/e-clock",
  "scripts": {
    "lint": "eslint ./src",
    "clear": "rimraf ./build && mkdirp ./build",
    "prepare": "cp -r ./src/img ./src/_locales ./src/manifest.json build",
    "prebuild": "npm run clear && npm run prepare",
    "build": "webpack --env dist",
    "prestart": "npm run clear && npm run prepare",
    "start": "webpack -w --env dev",
    "pretest": "npm run lint",
    "test": "echo No Tests"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "coffeelint": "^1.8.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "grunt": "^0.4.5",
    "grunt-coffeelint": "0.0.13",
    "grunt-contrib-clear": "^0.6.0",
    "grunt-contrib-coffee": "^0.12.0",
    "grunt-contrib-compress": "^0.9.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-stylus": "^0.20.0",
    "grunt-contrib-uglify": "^0.4.1",
    "grunt-contrib-watch": "^0.6.1",
    "html-webpack-plugin": "^2.22.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
