{
  "name": "e-clock",
  "version": "0.0.0",
  "description": "Chrome extension to replace \"new tab\" with digital clock",
  "repository": {
    "type": "git",
    "url": "https://github.com/husa/e-clock"
  },
  "author": "Yuriy Husnay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husa/e-clock/issues"
  },
  "homepage": "https://github.com/husa/e-clock",
  "scripts": {
    "lint": "eslint ./src",
    "clear": "rimraf ./build && mkdirp ./build",
    "prepare": "cp -r ./src/img ./src/_locales ./src/manifest.json ./src/analytics.js build",
    "prebuild": "npm run clear && npm run prepare",
    "build": "webpack --env dist -p",
    "postbuild": "rimraf ./*.zip && cd build && zip -r ../dist.zip *",
    "prestart": "npm run clear && npm run prepare",
    "start": "webpack -w --env dev",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register --recursive --require ./test/helper.js ./test",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
