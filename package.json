{
  "name": "e-clock",
  "version": "0.0.0",
  "description": "Chrome extension to replace \"new tab\" with digital clock",
  "repository": {
    "type": "git",
    "url": "https://github.com/husa/e-clock"
  },
  "author": "Yuriy Husnay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husa/e-clock/issues"
  },
  "homepage": "https://github.com/husa/e-clock",
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest --verbose",
    "clear": "rimraf ./build && mkdirp ./build",
    "start": "npm-run-all clear build:assets start:code",
    "start:code": "cross-env NODE_ENV=development webpack -w",
    "build": "npm-run-all clear build:*",
    "build:assets": "cp -r ./src/img ./src/fonts ./src/_locales ./src/manifest.json ./src/analytics.js ./build",
    "build:code": "cross-env NODE_ENV=production webpack",
    "zip": "bestzip build.zip ./build",
    "release": "node ./scripts/release.js",
    "deploy": "node ./scripts/deploy.js"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chrome-webstore-upload": "^0.2.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-react": "^7.16.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "flow-bin": "^0.109.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "semver": "^6.3.0",
    "simple-git": "^1.126.0",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "autobindr": "^1.0.6",
    "bestzip": "^2.1.4",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.1.0",
    "react": "^16.10.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
