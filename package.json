{
  "name": "e-clock",
  "version": "0.0.0",
  "description": "Chrome extension to replace \"new tab\" with digital clock",
  "repository": {
    "type": "git",
    "url": "https://github.com/husa/e-clock"
  },
  "author": "Yuriy Husnay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husa/e-clock/issues"
  },
  "homepage": "https://github.com/husa/e-clock",
  "scripts": {
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "jest --verbose",
    "clear": "rimraf ./build && mkdirp ./build",
    "start": "npm-run-all clear build:assets start:code",
    "start:code": "cross-env NODE_ENV=development webpack -w",
    "build": "npm-run-all clear build:*",
    "build:assets": "cp -r ./src/img ./src/fonts ./src/_locales ./src/manifest.json ./src/analytics.js ./build",
    "build:code": "cross-env NODE_ENV=production webpack",
    "zip": "bestzip build.zip ./build",
    "release": "node ./scripts/release.js",
    "deploy": "node ./scripts/deploy.js"
  },
  "devDependencies": {
    "@eleks/eslint-config-eleks": "^0.2.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chrome-webstore-upload": "^0.2.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.63.1",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.2",
    "jest-fetch-mock": "^1.4.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.5.4",
    "semver": "^5.4.1",
    "simple-git": "^1.85.0",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "autobindr": "^1.0.2",
    "bestzip": "^1.1.4",
    "classnames": "^2.2.5",
    "dotenv": "^4.0.0",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  }
}
