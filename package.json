{
  "name": "e-clock",
  "version": "0.0.0",
  "description": "Chrome extension to replace \"new tab\" with digital clock",
  "repository": {
    "type": "git",
    "url": "https://github.com/husa/e-clock"
  },
  "author": "Yuriy Husnay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husa/e-clock/issues"
  },
  "homepage": "https://github.com/husa/e-clock",
  "scripts": {
    "lint": "eslint ./src",
    "clear": "rimraf ./build && mkdirp ./build",
    "prepare": "cp -r ./src/img ./src/_locales ./src/manifest.json ./src/analytics.js build",
    "prebuild": "npm test && npm run clear && npm run prepare",
    "build": "cross-env NODE_ENV=production webpack -p",
    "postbuild1": "rimraf ./*.zip && cd build && zip -r ../dist.zip *",
    "prestart": "npm run clear && npm run prepare",
    "start": "cross-env NODE_ENV=develop webpack -w",
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/helper.js ./test",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@eleks/eslint-config-eleks": "^0.2.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  }
}
