{
  "name": "e-clock",
  "version": "0.0.0",
  "description": "Chrome extension to replace \"new tab\" with digital clock",
  "repository": {
    "type": "git",
    "url": "https://github.com/husa/e-clock"
  },
  "author": "husa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/husa/e-clock/issues"
  },
  "homepage": "https://github.com/husa/e-clock",
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest --verbose",
    "clear": "rimraf ./build && mkdirp ./build",
    "start": "npm-run-all clear build:assets start:code",
    "start:code": "cross-env NODE_ENV=development webpack -w",
    "build": "npm-run-all clear build:*",
    "build:assets": "cp -r ./src/img ./src/fonts ./src/_locales ./src/manifest.json ./src/analytics.js ./build",
    "build:code": "cross-env NODE_ENV=production webpack",
    "zip": "bestzip build.zip ./build",
    "release": "node ./scripts/release.js",
    "deploy": "node ./scripts/deploy.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,scss,md}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.6.3",
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chrome-webstore-upload": "^0.2.2",
    "cross-env": "^6.0.3",
    "css-loader": "^7.1.2",
    "eslint-plugin-react": "^7.34.2",
    "flow-bin": "^0.109.0",
    "globals": "^15.4.0",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^2.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^3.0.0",
    "sass": "^1.77.5",
    "sass-loader": "^14.2.1",
    "semver": "^6.3.0",
    "simple-git": "^1.126.0",
    "style-loader": "^1.3.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^8.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "autobindr": "^1.0.6",
    "bestzip": "^2.1.4",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.1.0",
    "eslint": "^8.57.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^7.1.1",
    "react-transition-group": "^4.4.5",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
}
